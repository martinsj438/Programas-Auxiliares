    function DynAdTvKeeper(container) {
        var idTimeout = null;
        var STATUS = {
                INITIALIZED : 0,
                LOADED: 1,
                PLAYING : 2,
                PAUSED: 3,
                CLOSED: 5,
                TIMEOUT: 4
            };

        var status = STATUS.INITIALIZED,
